<div class="row">
  <div class="col-md-12">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Landmark type:</strong>
      <%= @landmark.landmark_type_id %>
    </p>

    <p>
      <strong>Description:</strong>
      <%= @landmark.description %>
    </p>

    <p>
      <strong>Short description:</strong>
      <%= @landmark.short_description %>
    </p>

    <p>
      <strong>Picture url:</strong>
      <%= @landmark.picture_url %>
    </p>

    <p>
      <strong>Street Number:</strong>
      <%= landmark.street_number %>
    </p>

    <p>
      <strong>Street:</strong>
      <%= landmark.street %>
    </p>

    <p>
      <strong>City:</strong>
      <%= landmark.city %>
    </p>

    <p>
      <strong>State:</strong>
      <%= landmark.state %>
    </p>

    <p>
      <strong>Latitude:</strong>
      <%= landmark.latitude %>
    </p>

    <p>
      <strong>Longituds:</strong>
      <%= landmark.longitude %>
    </p>

    <%= link_to 'Edit', edit_landmark_path(@landmark), class: 'btn btn-default' %> |
    <%= link_to 'Back', landmarks_path, class: 'btn btn-default' %>
  </div>
</div>
